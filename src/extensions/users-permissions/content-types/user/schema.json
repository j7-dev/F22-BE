{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "vip": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::vip.vip",
      "inversedBy": "user"
    },
    "cash_balance": {
      "type": "decimal",
      "required": true
    },
    "reward_point_balance": {
      "type": "decimal",
      "required": true
    },
    "display_name": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "regex": "[0-9]"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "MALE",
        "FEMALE"
      ]
    },
    "birthday": {
      "type": "date"
    },
    "bank_accounts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bank-account.bank-account",
      "mappedBy": "user"
    },
    "vip_upgrate_records": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vip-upgrate-record.vip-upgrate-record",
      "mappedBy": "user"
    },
    "user_notes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-note.user-note",
      "mappedBy": "user"
    },
    "login_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::login-detail.login-detail",
      "mappedBy": "user"
    },
    "commission": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::commission.commission",
      "mappedBy": "user"
    },
    "transaction_records": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction-record.transaction-record",
      "mappedBy": "user"
    },
    "user_relationship": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-relationship.user-relationship",
      "mappedBy": "user"
    }
  }
}
