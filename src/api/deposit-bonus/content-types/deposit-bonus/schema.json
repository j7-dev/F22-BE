    "max_bonus_amount": {
      "type": "decimal",
      "min": 0,
      "required": false
    },
    "currency": {
      "type": "string",
      "required": true
    },
    "amount_type": {
      "type": "string",
      "required": true
    },
    "rolling_percentage": {
      "type": "decimal"
    },
    "allow_game_categories": {
      "type": "json"
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
