      "type": "decimal",
      "required": true
    },
    "game_id": {
      "type": "string",
      "required": true
    },
    "game_type": {
      "type": "string",
      "required": true
    },
    "game_table_id": {
      "type": "string",
      "required": true
    },
    "game_table_vid": {
      "type": "string",
      "required": true
    },
    "uuid": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "transaction_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "currency": {
      "type": "string"
