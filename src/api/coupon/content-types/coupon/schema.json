{
  "kind": "collectionType",
  "collectionName": "coupons",
  "info": {
    "singularName": "coupon",
    "pluralName": "coupons",
    "displayName": "Coupon",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "coupon_type": {
      "type": "enumeration",
      "enum": [
        "FIXED",
        "PERCENT"
      ],
      "default": "FIXED",
      "required": true
    },
    "coupon_amount": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "column": {
        "defaultTo": 0,
        "type": "decimal",
        "args": [
          16,
          6
        ]
      }
    },
    "currency": {
      "type": "string"
    },
    "amount_type": {
      "type": "string",
      "required": true
    },
    "is_claimed": {
      "type": "boolean",
      "default": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "AVAILABLE"
      ]
    }
  }
}
