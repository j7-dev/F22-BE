{
  "kind": "collectionType",
  "collectionName": "bet_records",
  "info": {
    "singularName": "bet-record",
    "pluralName": "bet-records",
    "displayName": "Bet Record",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "by": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "debit_amount": {
      "type": "decimal",
      "required": true
    },
    "credit_amount": {
      "type": "decimal"
    },
    "ref_id": {
      "type": "string",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "NORMAL",
        "PENDING",
        "CANCEL"
      ]
    },
    "bet_time": {
      "type": "datetime"
    },
    "update_time": {
      "type": "datetime"
    }
  }
}
