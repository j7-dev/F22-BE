{
  "kind": "collectionType",
  "collectionName": "bet_records",
  "info": {
    "singularName": "bet-record",
    "pluralName": "bet-records",
    "displayName": "Bet Record",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "DRAW",
        "WIN",
        "LOSE",
        "CANCEL",
        "GAMING"
      ],
      "required": true
    },
    "stake": {
      "type": "decimal",
      "required": true
    },
    "actual_stake": {
      "type": "decimal",
      "required": true
    },
    "winloss": {
      "type": "decimal",
      "required": false
    },
    "currency": {
      "type": "string",
      "required": true
    },
    "amount_type": {
      "type": "string",
      "required": true
    },
    "game_provider": {
      "type": "string",
      "required": true
    },
    "game_provider_transaction_id": {
      "type": "string"
    },
    "meta": {
      "type": "component",
      "repeatable": true,
      "component": "general.meta-data"
    },
    "is_finish": {
      "type": "boolean",
      "default": false
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
